<!doctype html>
<html>
  <head>
    <title>Movie Search Results</title>
    <link rel="stylesheet" href="style.css">
  </head>
  <body>
  	<h1>Movies:</h1>
  	<ul>
  		<% movieList.forEach(function (movie) { %>
  		<li><a href="movie/<%= movie.imdbID %>"> 
        <%= movie.Title %> </a></li>
  	<% }) %>
  </ul>

  </body>
</html>
